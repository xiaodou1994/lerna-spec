{
  "$schema": "https://raw.githubusercontent.com/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  "ul-style": "dash",
  "no-trailing-spaces": {
    "br_spaces": 0
  },
  "list-marker-space": false,
  "line-length": false,
  "no-inline-html": false,
  "no-duplicate-header": false,
  "proper-names": {
    "names": [
      "Javascript",
      "HTML"
    ],
    "code_blocks": false
  }
}